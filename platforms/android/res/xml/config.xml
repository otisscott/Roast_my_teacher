<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cstest.roastmyteacher" version="1.0.0" xmlns="http://www.w3.org/ns/widgets">
    <feature name="HeaderColor">
        <param name="android-package" value="tomloprod.HeaderColor" />
        <param name="onload" value="true" />
    </feature>
    <name>Roast my Teacher</name>
    <description>
        Give anonymous reviews for your teachers.
    </description>
    <icon density="mdpi" height="192" platform="android" src="www/img/logo.png" width="192" />
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="HeaderColor" value="#05bdd5" />
</widget>
